    "tags": ["material", "blog"],
    "author": {
      "name": "藍+85CD",
      "url": "https://kwaa.dev/"
    },
    "repo": "https://github.com/importantimport/lume_theme_shiraha",
    "demo": "https://lume.shiraha.js.org/",
    "screens": [
      {
        "desktop": "/screens/shiraha/desktop.png",
