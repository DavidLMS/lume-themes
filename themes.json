        "mobile": "/screens/shiraha/mobile.png",
        "mobile_dark": "/screens/shiraha/mobile_dark.png"
      }
    ],
    "module": {
      "name": "shiraha",
      "origin": "https://deno.land/x/lume_theme_shiraha",
      "main": "/mod.ts",
      "cms": "/_cms.ts",
      "src": [
        "/articles/getting-started.md",
        "/articles/part-1-installation.md",
        "/articles/part-1-writing.md"
      ],
      "compilerOptions": {
        "jsx": "precompile",
        "jsxImportSource": "shiraha",
        "jsxFactory": "jsx",
        "jsxFragmentFactory": "Fragment"
      },
      "imports": {
        "shiraha/jsx-runtime": "./jsx-runtime.ts"
      },
      "unstable": ["ffi"]
    }
  },
